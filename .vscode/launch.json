{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug Mocha test",
            "type": "node",
            "request": "launch",
            "program": "${workspaceRoot}/node_modules/webdriverio/bin/wdio",
            "stopOnEntry": true,
            "args":[
                "wdio.conf.js"
            ],
            "cwd": "${workspaceRoot}",
            "preLaunchTask": null,
            "runtimeExecutable": null,
            "runtimeArgs": [
                "--nolazy"
            ],
            "env": {
                "NODE_ENV": "development"
            },
            "externalConsole": false,
            "sourceMaps": false,
            "outDir": null
        }
    ]
}